<%- include ('E:/WebProjects/nodejs/PCAT/partials/_header.ejs') -%>


  <body>
    <div class="tm-page-wrap mx-auto">
      <div class="position-relative">
        <%- include ('E:/WebProjects/nodejs/PCAT/partials/_nav.ejs') -%>
        <div class="tm-welcome-container tm-fixed-header tm-fixed-header-3">
          <div class="text-center">
            <p
              class="pt-5 px-3 tm-welcome-text tm-welcome-text-2 mb-1 mt-lg-0 mt-5 text-white mx-auto"
            >
              Talk to Us<br />about any question you have
            </p>
          </div>
        </div>

        <div id="tm-fixed-header-bg"></div>
        <!-- Header image -->
      </div>

      <!-- Page content -->
      <main>
        <div class="container-fluid px-0">
          <div class="mx-auto tm-content-container">
            
            <div class="mx-auto pb-3 tm-about-text-container px-3">
              <div class="row">
                <div class="offset-1 col-lg-10 mb-5">
                  <form
                    id="contact-form"
                    action="/photos"
                    method="POST"
                    class="tm-contact-form"
                  >
                  <div class="form-group">
                    <input type="file" name="image" class="form-control-file rounded-0">
                  </div>
                    <div class="form-group">
                      <input
                        type="text"
                        name="title"
                        class="form-control rounded-0"
                        placeholder="Photo Title"
                        required
                      />
                    </div>
                    
                  
                    <div class="form-group">
                      <textarea
                        rows="8"
                        name="description"
                        class="form-control rounded-0"
                        placeholder="Message"
                        required
                      ></textarea>
                    </div>

                    <div class="form-group mb-0">
                      <button
                        type="submit"
                        class="btn btn-primary rounded-0 d-block ml-auto mr-0 tm-btn-animate tm-btn-submit tm-icon-submit"
                      >
                        <span>Submit</span>
                      </button>
                    </div>
                  </form>
                </div>
                
              </div>
            </div>
          </div>

          
          
        </div>
      </main>

      <div class="container-fluid tm-content-container mx-auto pt-4">
        <%- include ('E:/WebProjects/nodejs/PCAT/partials/_footer.ejs') -%>
      </div>
    </div>

    <script src="js/jquery-3.4.1.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/parallax.min.js"></script>
  </body>
</html>
